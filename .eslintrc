{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "linebreak-style": 0,
      "no-unused-vars": "warn",
      "consistent-return": "warn",
      "no-console": "off",
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    }
  }